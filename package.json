{
  "name": "codamit",
  "version": "1.0.0",
  "description": "Tech Blog",
  "engines": {
    "node": "^8.11.3"
  },
  "scripts": {
    "start": "npm run start:api:prod",
    "watch": "concurrently \"webpack-dev-server --config=webpack.dev.config.js --inline --hot\" \"nodemon --config nodemon.server.json\"",
    "prewatch": "npm install",
    "build:client": "webpack ---progress --config=webpack.production.config.js --mode production",
    "start:api:dev": "ts-node --require dotenv/config src/server/index.ts --config tsconfig.server.json dotenv_config_path=./.env",
    "start:api:prod": "ts-node --project tsconfig.server.json --require dotenv/config ./src/server/index.ts dotenv_config_path=.env.production",
    "deploy:api": "now -A now.api.json && now scale api.codamit.com bru1 1",
    "deploy:client": "npm run build:client && now -A now.client.json",
    "ln:api": "now ln -A now.api.json && now rm codamit-api --safe --yes",
    "ln:client": "now ln -A now.client.json && now rm codamit-client --safe --yes",
    "update:api": "npm run deploy:api && npm run ln:api",
    "update:client": "npm run deploy:client && npm run ln:client",
    "update:all": "concurrently \"npm run update:client\" \"npm run update:api\"",
    "lint": "npx eslint \"src/**/*.ts\"",
    "lint:fix": "npx eslint \"src/**/*.ts\" --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Edouardbozon/codamit.git"
  },
  "author": "edouardbozon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Edouardbozon/codamit/issues"
  },
  "homepage": "https://github.com/Edouardbozon/codamit#readme",
  "lint-staged": {
    "src/**/*.(ts|tsx|js|jsx)": [
      "npm run lint",
      "prettier --write",
      "git add"
    ],
    "src/**/*.(html|scss|json)": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@akanass/rx-http-request": "3.1.0",
    "@marblejs/core": "2.1.1",
    "@marblejs/middleware-body": "2.1.1",
    "@marblejs/middleware-cors": "2.1.1",
    "@marblejs/middleware-io": "2.1.1",
    "@marblejs/middleware-jwt": "2.1.1",
    "@marblejs/middleware-logger": "2.1.1",
    "@sentry/browser": "5.3.0",
    "@webcomponents/webcomponentsjs": "2.2.10",
    "anchorme": "1.1.2",
    "bcrypt": "3.0.6",
    "chalk": "2.4.2",
    "date-fns": "1.30.1",
    "dotenv": "8.0.0",
    "dotenv-webpack": "1.7.0",
    "lit-element": "2.1.0",
    "lit-html": "1.1.0",
    "mongoose": "5.5.11",
    "prismjs": "1.16.0",
    "prouter": "10.0.0",
    "pwa-helpers": "0.9.1",
    "redux": "4.0.1",
    "redux-observable": "1.1.0",
    "rxjs": "6.5.2",
    "showdown": "1.9.0",
    "simplemde": "1.11.2",
    "typegoose": "5.6.0",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-decorators": "7.4.4",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-typescript": "7.3.3",
    "@types/bcrypt": "3.0.0",
    "@types/express": "4.16.1",
    "@types/mongoose": "5.5.3",
    "@types/node": "12.0.2",
    "@types/prismjs": "1.16.0",
    "@types/showdown": "1.9.3",
    "@types/simplemde": "1.11.7",
    "@types/uuid": "3.4.4",
    "@typescript-eslint/eslint-plugin": "1.9.0",
    "@typescript-eslint/parser": "1.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-template-html-minifier": "3.0.1",
    "clean-webpack-plugin": "2.0.2",
    "concurrently": "4.1.0",
    "copy-webpack-plugin": "5.0.3",
    "css-loader": "2.1.1",
    "eslint": "5.16.0",
    "eslint-plugin-lit": "1.0.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "2.3.0",
    "lint-staged": "8.1.7",
    "meiosis-tracer": "2.0.3",
