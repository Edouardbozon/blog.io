{
  "name": "codamit",
  "version": "1.0.0",
  "description": "Tech Blog",
  "engines": {
    "node": "^8.11.3"
  },
  "scripts": {
    "start": "npm run start:api:prod",
    "watch": "concurrently \"webpack-dev-server --config=webpack.dev.config.js --inline --hot\" \"nodemon --config nodemon.server.json\"",
    "build:client": "webpack ---progress --config=webpack.production.config.js --mode production",
    "build:sw": "sw-precache --root='dist/client' --config=src/client/sw-config.js",
    "postbuild:client": "npm run build:sw",
    "start:api:dev": "ts-node --require dotenv/config src/server/index.ts --config tsconfig.server.json dotenv_config_path=./.env",
    "start:api:prod": "ts-node --project tsconfig.server.json --require dotenv/config ./src/server/index.ts dotenv_config_path=.env.production",
    "deploy:api": "now -A now.api.json && now scale api.codamit.com bru1 1",
    "deploy:client": "npm run build:client && now -A now.client.json",
    "ln:api": "now ln -A now.api.json && now rm codamit-api --safe --yes",
    "ln:client": "now ln -A now.client.json && now rm codamit-client --safe --yes",
    "update:all": "concurrently \"npm run update:client\" \"npm run update:api\"",
    "update:client": "npm run deploy:client && npm run ln:client",
    "update:api": "npm run deploy:api && npm run ln:api"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Edouardbozon/codamit.git"
  },
  "author": "edouardbozon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Edouardbozon/codamit/issues"
  },
  "homepage": "https://github.com/Edouardbozon/codamit#readme",
  "dependencies": {
    "@akanass/rx-http-request": "3.1.0",
    "@marblejs/core": "2.0.1",
    "@marblejs/middleware-body": "2.0.1",
    "@marblejs/middleware-io": "2.0.1",
    "@marblejs/middleware-joi": "2.0.1",
    "@marblejs/middleware-jwt": "2.0.1",
    "@marblejs/middleware-logger": "2.0.1",
    "@sentry/browser": "4.6.5",
    "@webcomponents/webcomponentsjs": "2.2.7",
    "anchorme": "1.1.2",
    "bcrypt": "3.0.5",
    "bulma": "0.7.4",
    "chalk": "2.4.2",
    "date-fns": "1.30.1",
    "dotenv": "7.0.0",
    "dotenv-webpack": "1.7.0",
    "express": "4.16.4",
    "flyd": "0.2.8",
    "lit-element": "2.1.0",
