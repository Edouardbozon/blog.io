{
  "name": "codamit-server",
  "author": "Edouard Bozon <bozonedouard@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --config nodemon.json",
    "build": "tsc --build tsconfig.json",
    "build:docker": "docker build -t codamit-server:latest .",
    "push": "heroku container:push web -a codamit-server",
    "prepush": "npm run build:docker",
    "release": "heroku container:release web -a codamit-server"
  },
  "dependencies": {
    "@akanass/rx-http-request": "3.2.0",
    "@marblejs/core": "2.3.1",
    "@marblejs/middleware-body": "2.3.1",
    "@marblejs/middleware-cors": "2.3.1",
    "@marblejs/middleware-io": "2.3.1",
    "@marblejs/middleware-jwt": "2.3.1",
    "@marblejs/middleware-logger": "2.3.1",
    "bcrypt": "3.0.6",
    "chalk": "2.4.2",
    "dotenv": "8.0.0",
    "mongoose": "5.6.4",
    "rxjs": "6.5.2",
    "typegoose": "5.8.0",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "ts-node": "8.3.0",
    "@types/bcrypt": "3.0.0",
    "@types/express": "4.17.0",
    "@types/mongoose": "5.5.8",
    "@types/node": "12.6.2",
    "@types/uuid": "3.4.5",
    "typescript": "3.5.3",
    "nodemon": "1.19.1"
  }
}
