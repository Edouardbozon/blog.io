{
  "name": "codamit-client",
  "version": 1,
  "type": "static",
  "static": {
    "public": "dist/client",
    "directoryListing": false,
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "**/*.@(jpg|jpeg|gif|png)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "s-maxage=259200"
          }
        ]
      },
      {
        "source": "**/*.@(js|css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "s-maxage=no-cache"
          }
        ]
      }
    ]
  },
  "alias": [
    "codamit.com",
    "www.codamit.com",
    "blog.codamit.com",
    "www.blog.codamit.com"
  ],
  "files": ["dist/client"]
}
